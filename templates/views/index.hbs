    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <link rel="stylesheet" href="/css/styles.css">
    
    
</head>

<body>
    <div>
        {{>header}}
        <form>
            <input type="text" placeholder="Enter A Character">
            
        </form>    
    </div>
    <main id="main">
        <div class="card">
